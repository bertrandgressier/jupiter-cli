{
  "name": "jupiter-cli",
  "version": "2.0.1",
  "description": "A secure, multi-wallet CLI for trading on Solana via Jupiter DEX aggregator",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "jupiter": "dist/index.js"
  },
  "files": [
    "dist/**/*",
    "prisma/schema.prisma",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --passWithNoTests",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "validate": "pnpm run typecheck && pnpm run lint && pnpm run format:check",
    "prepublishOnly": "pnpm run validate && pnpm run build && pnpm test",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prepare": "husky",
    "postinstall": "prisma generate",
    "semantic-release": "semantic-release",
    "semantic-release:dry-run": "semantic-release --dry-run"
  },
  "keywords": [
    "jupiter",
    "solana",
    "dex",
    "trading",
    "cli",
    "wallet",
    "cryptocurrency",
    "defi",
    "blockchain"
  ],
  "author": {
    "name": "Bertrand Gressier",
    "email": "bertrand.gressier@gmail.com",
    "url": "https://github.com/bertrandgressier"
  },
  "license": "MIT",
  "homepage": "https://github.com/bertrandgressier/jupiter-cli#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bertrandgressier/jupiter-cli.git"
  },
  "bugs": {
    "url": "https://github.com/bertrandgressier/jupiter-cli/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.2.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@prisma/client": "^6.3.1",
    "@solana/spl-token": "^0.4.12",
    "@solana/web3.js": "^1.98.0",
    "argon2": "^0.41.1",
    "axios": "^1.7.9",
    "big.js": "^6.2.2",
    "bs58": "^6.0.0",
    "chalk": "^4.1.2",
    "commander": "^13.1.0",
    "dotenv": "^16.4.7",
    "inquirer": "^12.4.1",
    "js-yaml": "^4.1.0",
    "ora": "^8.2.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "pino-roll": "^3.0.0",
    "tweetnacl": "^1.0.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/big.js": "^6.2.2",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.13.1",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.4.3",
    "nock": "^14.0.0",
    "prettier": "^3.4.2",
    "prisma": "^6.3.1",
    "semantic-release": "^24.2.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}
